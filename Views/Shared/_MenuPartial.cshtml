@using Microsoft.AspNetCore.Identity
@inject SignInManager<IdentityUser> SignInManager
@inject UserManager<IdentityUser> UserManager

    
@if (SignInManager.IsSignedIn(User))
{
    <li class="nav-item">
        <a class="nav-link text-primary" asp-area="" asp-controller="Producto"
          asp-action="Index">Productos</a>
    </li>
    <li class="nav-item">
        <a class="nav-link text-primary" asp-area="" asp-controller="Home"
          asp-action="Index">Carrito</a>
    </li>
    <li class="nav-item">
         <a class="nav-link text-primary" asp-area="" asp-controller="Contacto" asp-action="Index">Lista Contactos</a>
     </li>
     <li class="nav-item">
         <a class="nav-link text-primary" asp-area="" asp-controller="Registro" asp-action="Registro">Datos</a>
     </li>
}
else{
    <li class="nav-item">
        <a class="nav-link text-primary" asp-area="" asp-controller="Home"
          asp-action="Privacy">Tienda</a>
    </li>
    <li class="nav-item">
        <a class="nav-link text-primary" asp-area="" asp-controller="Contacto" asp-action="Create">Contactenos</a>
     </li>
}

